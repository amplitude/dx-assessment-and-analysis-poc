{
  "compilerOptions": {
    "alwaysStrict": false,
    "baseUrl": ".",
    "declaration": true,
    "declarationMap": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "inlineSources": true,
    "importHelpers": true,
    "lib": [
      "es6"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitHelpers": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitUseStrict": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "pretty": true,
    "rootDir": ".",
    "resolveJsonModule": true,
    "sourceMap": true,
    "strict": true,
    "strictBindCallApply": true,
    "target": "es5",
    "types": [
      "jest",
      "node"
    ],
    "paths": {
      "hub": [
        "packages/hub/src/index.ts"
      ]
    }
  }
}
